{
  "name": "root",
  "version": "0.0.0",
  "description": "lightweight and fast css-in-js solution",
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "node -r esbuild-register scripts/build.ts",
    "build:doc": "cd docs && pnpm run build",
    "build:plugin": "node -r esbuild-register scripts/buildPlugin.ts",
    "test": "jest-easy",
    "test:coverage": "jest-easy --coverage",
    "update:deps": "pnpm update --latest --interactive",
    "format": "prettier \"**/*.{js,ts,tsx,md}\" --write",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "release": "node -r esbuild-register scripts/release.ts",
    "type:react": "tsc -p specs/react/tsconfig.json",
    "type:vue": "tsc -p specs/vue/tsconfig.json",
    "type:solid": "tsc -p specs/solid/tsconfig.json",
    "typescript": "tsc -p tsconfig.json && pnpm run type:solid && pnpm run type:react && pnpm run type:vue"
  },
  "keywords": [
    "css-in-js",
    "styils",
    "styled",
    "emotion",
    "styled-components",
    "javascript",
    "react",
    "preact"
  ],
  "author": "zoy-l <zoy-l@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@commitlint/cli": "17.4.3",
    "@commitlint/config-conventional": "17.4.3",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@solidjs/testing-library": "^0.6.1",
    "@swc/core": "1.3.44",
    "@swc/jest": "0.2.24",
    "@testing-library/react": "14.0.0",
    "@types/babel__core": "7.20.0",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@types/prettier": "2.7.2",
    "@types/react": "17.0.56",
    "@types/react-dom": "17.0.19",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vue/test-utils": "2.2.10",
    "esbuild": "0.17.19",
    "esbuild-register": "3.4.2",
    "eslint": "8.34.0",
    "eslint-config-iroot": "1.4.6",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-easy": "1.0.3",
    "jest-environment-jsdom": "29.4.3",
    "ora": "6.1.2",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "4.1.2",
    "rollup": "3.15.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-esbuild": "5.0.0",
    "shelljs": "0.8.5",
    "solid-js": "1.6.11",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vue": "3.2.47"
  },
  "engines": {
    "node": ">=12",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "react-is/*",
        "vite",
        "vue"
      ]
    }
  }
}
